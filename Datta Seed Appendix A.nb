Cell[TextData[{
 "The difference in column j between the entry for the i+1th row and the i th \
row will be ",
 Cell[BoxData[
  RowBox[{
   RowBox[{
    FractionBox[
     SuperscriptBox[
      RowBox[{"(", 
       RowBox[{"-", "1"}], ")"}], 
      RowBox[{"j", "+", "i", "+", "1"}]], 
     RowBox[{"2", "\[Pi]", " ", "\[ImaginaryI]"}]], 
    RowBox[{"(", " ", 
     RowBox[{
      UnderoverscriptBox["\[Product]", 
       RowBox[{"r", "=", "j"}], "i"], 
      SubscriptBox["\[Epsilon]", "r"]}], ")"}], 
    RowBox[{"\[ContourIntegral]", 
     FractionBox[
      RowBox[{
       SuperscriptBox["\[ExponentialE]", 
        RowBox[{
         RowBox[{"-", "t"}], " ", "s"}]], "  ", 
       RowBox[{"\[DifferentialD]", "s", " "}]}], 
      RowBox[{
       SubsuperscriptBox["\[Product]", 
        RowBox[{"m", "=", "j"}], 
        RowBox[{"i", "+", "1"}]], 
       RowBox[{"(", 
        RowBox[{"s", "-", 
         SubscriptBox["\[Epsilon]", "m"]}], ")"}]}]]}]}], "-", 
   RowBox[{
    FractionBox[
     SuperscriptBox[
      RowBox[{"(", 
       RowBox[{"-", "1"}], ")"}], 
      RowBox[{"j", "+", "i"}]], 
     RowBox[{"2", "\[Pi]", " ", "\[ImaginaryI]"}]], 
    RowBox[{"(", " ", 
     RowBox[{
      UnderoverscriptBox["\[Product]", 
       RowBox[{"r", "=", "j"}], 
       RowBox[{"i", "-", "1"}]], 
      SubscriptBox["\[Epsilon]", "r"]}], ")"}], 
    RowBox[{"\[ContourIntegral]", 
     FractionBox[
      RowBox[{
       SuperscriptBox["\[ExponentialE]", 
        RowBox[{
         RowBox[{"-", "t"}], " ", "s"}]], "  ", 
       RowBox[{"\[DifferentialD]", "s", " "}]}], 
      RowBox[{
       SubsuperscriptBox["\[Product]", 
        RowBox[{"m", "=", "j"}], "i"], 
       RowBox[{"(", 
        RowBox[{"s", "-", 
         SubscriptBox["\[Epsilon]", "m"]}], ")"}]}]]}]}]}]],
  CellChangeTimes->{{3.4270546634375*^9, 3.427054720296875*^9}},
  ExpressionUUID->"49bcdd31-d790-4593-9fa1-ce7ea8a03b47"],
 ", where the contour encloses all of the zeroes of the denominator. The \
first contour will enclose the second so we can combine the integrals, giving \
",
 Cell[BoxData[
  RowBox[{
   FractionBox[
    SuperscriptBox[
     RowBox[{"(", 
      RowBox[{"-", "1"}], ")"}], 
     RowBox[{"j", "+", "i", "+", "1"}]], 
    RowBox[{"2", "\[Pi]", " ", "\[ImaginaryI]"}]], 
   RowBox[{"(", " ", 
    RowBox[{
     UnderoverscriptBox["\[Product]", 
      RowBox[{"r", "=", "j"}], 
      RowBox[{"i", "-", "1"}]], 
     SubscriptBox["\[Epsilon]", "r"]}], ")"}], 
   RowBox[{"(", 
    RowBox[{"\[ContourIntegral]", 
     FractionBox[
      RowBox[{
       SuperscriptBox["\[ExponentialE]", 
        RowBox[{
         RowBox[{"-", "t"}], " ", "s"}]], " ", 
       RowBox[{"(", 
        RowBox[{"s", "-", 
         RowBox[{"(", 
          RowBox[{
           SubscriptBox["\[Epsilon]", 
            RowBox[{"i", "+", "1"}]], "-", 
           SubscriptBox["\[Epsilon]", "i"]}], ")"}]}], ")"}], 
       RowBox[{"\[DifferentialD]", "s", " "}]}], 
      RowBox[{
       SubsuperscriptBox["\[Product]", 
        RowBox[{"m", "=", "j"}], 
        RowBox[{"i", "+", "1"}]], 
       RowBox[{"(", 
        RowBox[{"s", "-", 
         SubscriptBox["\[Epsilon]", "m"]}], ")"}]}]]}], ")"}]}]],
  CellChangeTimes->{{3.4270546634375*^9, 3.427054720296875*^9}, {
