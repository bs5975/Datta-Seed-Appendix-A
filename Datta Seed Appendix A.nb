Cell[BoxData[
 RowBox[{
  RowBox[{
   RowBox[{"W", "[", 
    RowBox[{"d_", ",", "n_"}], "]"}], ":=", 
   RowBox[{"Table", "[", 
    RowBox[{
     RowBox[{"If", "[", 
      RowBox[{
       RowBox[{"i", "<", "j"}], ",", "0", ",", 
       RowBox[{
        SuperscriptBox[
         RowBox[{"(", 
          RowBox[{"-", "1"}], ")"}], 
         RowBox[{"i", "+", "j"}]], 
        RowBox[{"(", " ", 
         RowBox[{
          UnderoverscriptBox["\[Product]", 
           RowBox[{"k", "=", "j"}], 
           RowBox[{"i", "-", "1"}]], 
          SubscriptBox["\[Epsilon]", "k"]}], ")"}], 
        RowBox[{"dvdf", "[", 
         RowBox[{
          RowBox[{
           SuperscriptBox[
            RowBox[{"(", 
             RowBox[{"-", "#"}], ")"}], "n"], "&"}], ",", "\[Epsilon]", ",", 
          "j", ",", "i"}], "]"}]}]}], "]"}], ",", 
     RowBox[{"{", 
      RowBox[{"i", ",", "1", ",", "d"}], "}"}], ",", 
     RowBox[{"{", 
      RowBox[{"j", ",", "1", ",", "d"}], "}"}]}], "]"}]}], ";", 
  RowBox[{
   RowBox[{"Wt", "[", "d_", "]"}], ":=", 
   RowBox[{"Table", "[", 
    RowBox[{
