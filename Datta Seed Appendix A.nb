Cell[TextData[{
 "The moments of order ",
 StyleBox["m",
  FontSlant->"Italic"],
 " are defined formally by ",
 Cell[BoxData[
  FormBox[
   RowBox[{
    SubscriptBox["lim", 
     RowBox[{"d", "\[Rule]", " ", "\[Infinity]"}]], 
    RowBox[{
     SubsuperscriptBox["\[Sum]", 
      RowBox[{"n", "=", "0"}], "d"], 
     RowBox[{
      SubsuperscriptBox["\[Sum]", 
       RowBox[{"i", "=", "1"}], "d"], 
      RowBox[{
       SuperscriptBox[
        RowBox[{"(", 
         RowBox[{"i", "-", "1"}], ")"}], "m"], 
       RowBox[{
        FractionBox[
         SuperscriptBox["T", "n"], 
         RowBox[{"n", "!"}]], "[", 
        RowBox[{"[", 
         RowBox[{"i", ",", "1"}], "]"}], "]"}]}]}]}]}], TraditionalForm]],
  ExpressionUUID->"2354e496-fe4c-4e8d-a15e-3d65fa685706"],
 ". For convenience the moment definition ",
 Cell[BoxData[
  FormBox[
   RowBox[{"Ym", "[", 
    RowBox[{"m", ",", "d", ",", "n"}], "]"}], TraditionalForm]],
  ExpressionUUID->"5708c6e4-e45a-4605-9bbd-f1357b1008e9"],
 " \[Congruent] ",
 Cell[BoxData[
  FormBox[
   RowBox[{
    SubsuperscriptBox["\[Sum]", 
     RowBox[{"i", "=", "1"}], "d"], 
    RowBox[{
     SuperscriptBox[
      RowBox[{"(", 
       RowBox[{"i", "-", "1"}], ")"}], "m"], 
     RowBox[{
      SuperscriptBox["T", "n"], "[", 
      RowBox[{"[", 
       RowBox[{"i", ",", "1"}], "]"}], "]"}]}]}], TraditionalForm]],
  ExpressionUUID->"832fe454-1fb0-45db-b997-8458e277dfe4"],
 " can be adopted. ",
 Cell[BoxData[
  FormBox[
   RowBox[{" ", 
    RowBox[{
     RowBox[{"Ym", "[", 
      RowBox[{"m", ",", "d", ",", "1"}], "]"}], "=", 
     SubscriptBox["\[Epsilon]", "1"]}]}], TraditionalForm]],ExpressionUUID->
  "4a4c1b61-ceb4-46fc-819c-1782c7a8ae86"],
 ",  ",
 Cell[BoxData[
  FormBox[
   RowBox[{
    RowBox[{"Ym", "[", 
     RowBox[{"m", ",", "d", ",", "2"}], "]"}], "=", 
    RowBox[{
     RowBox[{"-", 
      SubsuperscriptBox["\[Epsilon]", "1", "2"]}], "+", 
     RowBox[{
      RowBox[{"(", 
       RowBox[{
        SuperscriptBox["2", "m"], "-", "1"}], ")"}], 
      SubscriptBox["\[Epsilon]", "1"], 
      SubscriptBox["\[Epsilon]", "2"]}]}]}], TraditionalForm]],
  ExpressionUUID->"f32a7ffd-93d4-4a53-bd19-736cff2aed20"],
 ". For ",
 StyleBox["d",
  FontSlant->"Italic"],
 " > ",
 StyleBox["n",
  FontSlant->"Italic"],
 " > 2, ",
 Cell[BoxData[
  RowBox[{
   RowBox[{"Ym", "[", 
    RowBox[{"m", ",", "d", ",", "n"}], "]"}], "=", 
   RowBox[{
    SuperscriptBox[
     RowBox[{"(", 
      RowBox[{"-", "1"}], ")"}], "n"], 
    RowBox[{
     SubsuperscriptBox["\[Sum]", 
      RowBox[{"k", "=", "1"}], 
      RowBox[{"n", "-", "1"}]], 
     RowBox[{"(", 
      RowBox[{
       RowBox[{
        SubscriptBox["X", 
         RowBox[{"k", ",", "m"}]], "[", "\[Epsilon]", "]"}], 
       RowBox[{"dvdf", "[", 
        RowBox[{
         RowBox[{
          SuperscriptBox["#", 
           RowBox[{"n", "-", "3"}]], " ", "&"}], ",", "\[Epsilon]", ",", "1", 
         ",", "k"}], "]"}], 
       RowBox[{
        SubsuperscriptBox["\[Product]", 
         RowBox[{"j", "=", "1"}], "k"], 
        SubscriptBox["\[Epsilon]", "j"]}]}], ")"}]}]}]}]],
  CellChangeTimes->{{3.5877623874849467`*^9, 3.5877624413770294`*^9}, {
   3.5877625247267966`*^9, 3.587762528440009*^9}, {3.5877630799585543`*^9, 
   3.5877630830997334`*^9}},ExpressionUUID->
  "4de59cb0-c3e0-4967-9860-58d9f1eac969"],
 ", with ",
 Cell[BoxData[
  FormBox[
   SubscriptBox["X", 
    RowBox[{"k", ",", "m"}]], TraditionalForm]],ExpressionUUID->
  "6e028430-e3e1-4527-8e1a-ced4b43a5835"],
 " defined as below."
}], "Text",
 CellChangeTimes->{{3.5877629249206867`*^9, 3.5877630049752655`*^9}, {
