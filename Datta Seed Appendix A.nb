 Cell[BoxData[
  RowBox[{
   FractionBox["1", 
    RowBox[{"2", "\[Pi]", " ", "\[ImaginaryI]"}]], 
   RowBox[{"\[ContourIntegral]", 
    FractionBox[
     RowBox[{
      SuperscriptBox["s", "n"], "\[Lambda]", 
      RowBox[{"(", 
       RowBox[{
        SubsuperscriptBox["\[Product]", 
         RowBox[{"k", "=", 
          RowBox[{"i", "+", "1"}]}], 
         RowBox[{"j", "-", "1"}]], 
        RowBox[{"(", 
         RowBox[{"s", "+", 
          SubscriptBox["\[Epsilon]", "k"]}], ")"}]}], ")"}], 
      RowBox[{"(", 
       RowBox[{
        SubsuperscriptBox["\[Product]", 
         RowBox[{"m", "=", "j"}], "d"], 
        SubscriptBox["\[Epsilon]", "m"]}], ")"}], 
      RowBox[{"(", 
       RowBox[{
        SubsuperscriptBox["\[Product]", 
         RowBox[{"m", "=", "1"}], 
         RowBox[{"i", "-", "1"}]], 
        SubscriptBox["\[Epsilon]", "m"]}], ")"}]}], 
     RowBox[{
      RowBox[{
       SubsuperscriptBox["\[Product]", 
        RowBox[{"k", "=", "1"}], "d"], 
       RowBox[{"(", 
        RowBox[{"s", "+", 
         SubscriptBox["\[Epsilon]", "k"]}], ")"}]}], "-", 
      RowBox[{"\[Lambda]", 
       RowBox[{
        SubsuperscriptBox["\[Product]", 
         RowBox[{"m", "=", "1"}], "d"], 
        SubscriptBox["\[Epsilon]", "m"]}]}]}]]}]}]],
  CellChangeTimes->{{3.5597381189714756`*^9, 3.5597381314331884`*^9}, {
   3.7215823705665913`*^9, 3.7215824078138704`*^9}, {3.7216034268265133`*^9, 
